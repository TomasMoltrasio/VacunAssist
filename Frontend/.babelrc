    "presets": [
        "@babel/preset-env",
        ["@babel/preset-react", {"runtime": "automatic"}]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        ["import", { 
            "libraryName": "antd",
            "libraryDirectory": "lib",  // libraryDirectory por defecto es lib
